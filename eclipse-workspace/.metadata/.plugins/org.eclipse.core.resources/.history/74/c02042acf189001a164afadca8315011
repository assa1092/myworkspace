package com.naver;

public class Monster {
	private int hp;
	
	
	
	public Monster(int hp) {
		super();
		this.hp = hp;
	}

	public Monster() {
		// TODO Auto-generated constructor stub
	}

	public int getHp() {
		return hp;
	}

	public void setHp(int hp) {
		this.hp = hp;
	}
	
	// 몬스터 객체
	@Override
	public String toString() {
		return "Monster [hp=" + hp + "]";
	}
	
	

}
